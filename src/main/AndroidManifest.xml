<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.AttendanceSystem">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AttendanceSystem">
        <activity
            android:name="com.example.attendancesystem.saveCourse"
            android:exported="false" />
        <activity
            android:name="com.example.attendancesystem.StudentMeetingActivity"
            android:exported="false" />
        <activity
            android:name="com.example.attendancesystem.StudentCourseActivity"
            android:exported="false" />
        <activity
            android:name="com.example.attendancesystem.AttendanceActivity"
            android:exported="false" />
        <activity
            android:name="com.example.attendancesystem.MeetingActivity"
            android:exported="false" />
        <activity
            android:name="com.example.attendancesystem.CourseActivity"
            android:exported="false" />
        <activity
            android:name="com.example.attendancesystem.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>